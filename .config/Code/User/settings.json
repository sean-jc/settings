// Place your settings in this file to overwrite the default settings
{
    "editor.fontSize": 12,
    "editor.tabSize": 4,

    "files.eol": "\n",

    "files.exclude": {
        "**/.vscode":true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.a": true,
        "**/*.o": true,
        "**/*.so": true
    },
    "files.watcherExclude": {
        "**/.vscode":true,
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        "**/*.a": true,
        "**/*.o": true,
        "**/*.so": true
    },
    "search.exclude": {
        "**/.vscode":true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.a": true,
        "**/*.o": true,
        "**/*.so": true
    }
,
"window.zoomLevel": 0,
"workbench.colorTheme": "Monokai Dark Soda",
"[cpp]": {
        "editor.quickSuggestions": false
},
"[c]": {
        "editor.quickSuggestions": false
}
}