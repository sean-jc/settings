#!/bin/bash
if [[ $# -ne 2 ]]; then
    printf "Must specify the source build and target VM\n"
    exit 1
fi
rm -f ~/build/kernel/$1/lib/modules/4.14.0+/build
rm -f ~/build/kernel/$1/lib/modules/4.14.0+/source
scp ~/build/kernel/$1/arch/x86/boot/bzImage $2:/home/sean/Downloads/gtest/vmlinuz-4.14.0-gtest+
scp ~/build/kernel/$1/System.map $2:/home/sean/Downloads/gtest/System.map-4.14.0-gtest+
scp -r ~/build/kernel/$1/lib/modules/4.14.0+ $2:/home/sean/Downloads/gtest/4.14.0-gtest+
